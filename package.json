{
  "name": "trickcord-treat",
  "version": "2.0.0",
  "author": "HeadTriXz",
  "repository": "HeadTriXz/Trickcord-Treat",
  "description": "This is a Halloween-themed Discord bot that sends virtual trick-or-treaters to your server for a speed-based game.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.10.0"
  },
  "packageManager": "npm@10.5.0",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "start": "dotenv -- node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.20.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@barry-bot/core": "^1.1.1",
    "@barry-bot/logger": "^1.0.2",
    "@discordjs/core": "^2.0.0",
    "@discordjs/rest": "^2.4.0",
    "@discordjs/ws": "^2.0.0",
    "@prisma/client": "^5.20.0"
  }
}
